{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://api.xtaze.fun/:path*"
      }
    ],
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://xtaze.fun" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
        ]
      }
    ]
  }
  